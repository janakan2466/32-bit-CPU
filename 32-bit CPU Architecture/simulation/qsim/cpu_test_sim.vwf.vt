wire [31:0] outPC;
wire outZ;
wire [2:0] T_Info;
wire wEn;
wire wen_mem;

wire sampler;                             

// assign statements (if any)                          
CPU_TEST_Sim i1 (
// port map - connection between master ports and signals/registers   
	.addrOut(addrOut),
	.cpuClk(cpuClk),
	.en_mem(en_mem),
	.memClk(memClk),
	.memDataIn(memDataIn),
	.memDataOut(memDataOut),
	.outA(outA),
	.outB(outB),
